{
  "name": "postcss-positioning",
  "version": "0.1.1",
  "description": "Make CSS positioning ridiculously simple",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "positioning",
    "spacing"
  ],
  "author": "Matthias Totschnig <matthias@totschnig.org>",
  "license": "MIT",
  "repository": "matthias-t/postcss-positioning",
  "bugs": {
    "url": "https://github.com/matthias-t/postcss-positioning/issues"
  },
  "homepage": "https://github.com/matthias-t/postcss-positioning",
  "dependencies": {
    "postcss": "^6.0.16"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-object-entries-and-values-to-object-keys": "^1.0.1",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.17.0",
    "eslint-config-postcss": "^2.0.2",
    "eslint-plugin-import": "^2.8.0",
    "jest": "^21.0.0"
  },
  "peerDependencies": {
    "postcss-autoreset": "^2.0.2",
    "postcss-calc": "^6.0.1"
  },
  "scripts": {
    "build": "babel src --out-dir js",
    "test": "npm run build && jest --verbose --coverage && eslint \"src/**.js\"",
    "prepublishOnly": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-postcss/es5",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "plugins": [
      "import"
    ],
    "env": {
      "jest": true
    }
  }
}
